(require lispy.macros *)

(def count-for-digit [d]
  (= lower 1)
  (= upper 10)
  (= n d)
  (= cnt 0)
  (while (and (<= lower n) (< n upper))
    (+= cnt 1)
    (*= lower 10)
    (*= upper 10)
    (*= n d))
  (return cnt))

(def sol []
  (= rst 0)
  (for i (range 1 10)
    (+= rst (count-for-digit i)))
  (return rst))

(print (sol))
