(require lispy.macros *)

(def all-primes-till [n]
  (= sieve (+ (* (min 2 n) [0]) (* (- n 1) [1])))
  (for i (range 2 (+ n 1))
    (for j (range (* 2 i) (+ n 1) i)
      (= (sub sieve j) 0)))
  (return [i for i in (range (+ n 1)) if (sub sieve i)]))

(def sol [n]
  (= ret 1)
  (for p (all-primes-till n)
    (= f 1)
    (while (<= f (/ n p))
      (*= f p))
    (*= ret f))
  (return ret))

(print (sol 20))
